{
    "name": "Summarizer",
    "description": "Extension for summarizing online meetings!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "script.js"
      },
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html"
      }
  }